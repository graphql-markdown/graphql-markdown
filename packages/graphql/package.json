{
  "name": "@graphql-markdown/graphql",
  "homepage": "https://graphql-markdown.dev",
  "description": "GraphQL-Markdown GraphQL utilities.",
  "bugs": {
    "url": "https://github.com/graphql-markdown/graphql-markdown/issues"
  },
  "version": "1.1.9",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphql-markdown/graphql-markdown.git",
    "directory": "packages/graphql"
  },
  "author": {
    "name": "Gregory Heitz"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./*": {
      "require": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      },
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      },
      "default": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "stryker": "stryker run",
    "clean": "tsc --build --clean && rm -rf ./dist",
    "docs": "typedoc",
    "build": "tsc --build",
    "pack": "bun pm pack"
  },
  "dependencies": {
    "@graphql-tools/load": "^8.1.2",
    "@graphql-markdown/utils": "workspace:^"
  },
  "peerDependencies": {
    "graphql": "^16.0"
  },
  "devDependencies": {
    "@graphql-markdown/types": "workspace:^"
  },
  "directories": {
    "test": "tests"
  },
  "engines": {
    "node": ">=16.14"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "files": [
    "dist"
  ]
}
