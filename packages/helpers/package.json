{
  "name": "@graphql-markdown/helpers",
  "homepage": "https://graphql-markdown.dev",
  "description": "GraphQL-Markdown customization helpers.",
  "bugs": {
    "url": "https://github.com/graphql-markdown/graphql-markdown/issues"
  },
  "version": "1.0.12",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphql-markdown/graphql-markdown.git",
    "directory": "packages/helpers"
  },
  "author": {
    "name": "Gregory Heitz"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./directives/*": {
      "require": {
        "types": "./dist/directives/**.d.ts",
        "default": "./dist/directives/**.js"
      },
      "import": {
        "types": "./dist/directives/**.d.ts",
        "default": "./dist/directives/**.js"
      },
      "default": {
        "types": "./dist/directives/**.d.ts",
        "default": "./dist/directives/**.js"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "stryker": "stryker run",
    "clean": "tsgo --build --clean && rm -rf ./dist",
    "docs": "typedoc",
    "build": "tsgo --build",
    "pack": "bun pm pack"
  },
  "dependencies": {
    "@graphql-markdown/graphql": "workspace:^",
    "@graphql-markdown/utils": "workspace:^"
  },
  "peerDependencies": {
    "graphql": "^16.0"
  },
  "devDependencies": {
    "@graphql-markdown/types": "workspace:^"
  },
  "directories": {
    "test": "tests"
  },
  "engines": {
    "node": ">=16.14"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "files": [
    "dist"
  ]
}
