{
  "name": "@graphql-markdown/core",
  "homepage": "https://graphql-markdown.dev",
  "description": "GraphQL-Markdown core package for generating Markdown documentation from a GraphQL schema.",
  "bugs": {
    "url": "https://github.com/graphql-markdown/graphql-markdown/issues"
  },
  "version": "1.16.3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphql-markdown/graphql-markdown.git",
    "directory": "packages/core"
  },
  "author": {
    "name": "Gregory Heitz"
  },
  "keywords": [
    "graphql",
    "graphql-schema",
    "documentation",
    "markdown",
    "graphql-markdown"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./*": {
      "require": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      },
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      },
      "default": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "stryker": "stryker run",
    "clean": "rm -rf ./dist",
    "docs": "typedoc",
    "suggest:tests": "ts-node ../../scripts/test-suggestions.ts"
  },
  "dependencies": {
    "@fastify/deepmerge": "^3.1.0",
    "@graphql-markdown/graphql": "^1.1.9",
    "@graphql-markdown/logger": "^1.0.5",
    "@graphql-markdown/utils": "^1.9.0"
  },
  "peerDependencies": {
    "@graphql-markdown/diff": "^1.1.13",
    "@graphql-markdown/helpers": "^1.0.12",
    "@graphql-markdown/printer-legacy": "^1.12.3",
    "graphql-config": "^5.1.0"
  },
  "devDependencies": {
    "@graphql-markdown/types": "^1.8.0"
  },
  "peerDependenciesMeta": {
    "@graphql-markdown/printer-legacy": {
      "optional": true
    },
    "@graphql-markdown/diff": {
      "optional": true
    },
    "@graphql-markdown/helpers": {
      "optional": true
    },
    "graphql-config": {
      "optional": true
    }
  },
  "directories": {
    "test": "tests"
  },
  "engines": {
    "node": ">=16.14"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "files": [
    "dist",
    "assets"
  ]
}
