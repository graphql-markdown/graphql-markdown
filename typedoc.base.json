{
  "$schema": "https://typedoc.org/schema.json",
  "plugin": ["typedoc-plugin-markdown"],
  "exclude": ["**/index.ts"],
  "excludeExternals": true,
  "excludeInternal": false,
  "excludePrivate": true,
  "excludeReferences": true,
  "excludeNotDocumented": false,
  "githubPages": false,
  "hideBreadcrumbs": true,
  "hideGenerator": true,
  "hidePageHeader": true,
  "hidePageTitle": false,
  "useCodeBlocks": true,
  "outputFileStrategy": "modules",
  "pretty": true,
  "theme": "default",
  "validation": {
    "notExported": false,
    "invalidLink": false,
    "rewrittenLink": false,
    "notDocumented": false,
    "unusedMergeModuleWith": false
  },
  "visibilityFilters": {},
  "sourceLinkExternal": true,
  "gitRevision": "main",
  "entryFileName": "index",
  "excludeTags": ["@alias"]
}
