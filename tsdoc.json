{
  "$schema": "https://developer.microsoft.com/json-schemas/tsdoc/v0/tsdoc.schema.json",
  "noStandardTags": false,
  "tagDefinitions": [
    {
      "tagName": "@alias",
      "syntaxKind": "block"
    },
    {
      "tagName": "@static",
      "syntaxKind": "modifier"
    },
    {
      "tagName": "@useDeclaredType",
      "syntaxKind": "modifier"
    },
    {
      "tagName": "@module",
      "syntaxKind": "block"
    },
    {
      "tagName": "@category",
      "syntaxKind": "block",
      "allowMultiple": true
    },
    {
      "tagName": "@primaryExport",
      "syntaxKind": "modifier"
    },
    {
      "tagName": "@interface",
      "syntaxKind": "modifier"
    },
    {
      "tagName": "@since",
      "syntaxKind": "block"
    },
    {
      "tagName": "@property",
      "syntaxKind": "block",
      "allowMultiple": true
    },
    {
      "tagName": "@private",
      "syntaxKind": "modifier"
    },
    {
      "tagName": "@template",
      "syntaxKind": "block",
      "allowMultiple": true
    },
    {
      "tagName": "@abstract",
      "syntaxKind": "modifier"
    }
  ]
}
